{
  "name": "autochore",
  "private": true,
  "scripts": {
    "api": "NODE_ENV='development' node lib/mocks/mock-api-server.js",
    "bc": "yarn buildcom",
    "build": "next build",
    "dev": "next",
    "dev-local": "run-p api dev",
    "start": "next start",
    "test": "run-s test:*",
    "test:ts": "tsc",
    "test:audit": "yarn improved-yarn-audit --min-severity moderate --ignore-dev-deps",
    "lint": "run-s lint:*",
    "lint:md": "markdownlint -c .markdownlint.json --fix --ignore node_modules **/*.md",
    "lint:json": "eslint **/*.json --fix",
    "lint:styles": "stylelint \"./components/**/*.(s)css\" \"./pages/**/*.scss\" \"./templates/**/*.scss\" \"./themes/**/*.scss\" --fix",
    "lint:ts": "eslint-output \"./components\" \"./pages\" \"./templates\" --ext .ts,.tsx --fix",
    "test:unit": "cross-env NODE_ICU_DATA=node_modules/full-icu jest --verbose",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "precommit-msg": "echo 'Performing pre-commit checks...' && exit 0",
    "gendocs": "node ./generate-docs.js",
    "genreg": "node ./generate-registries.js",
    "postinstall": "husky install",
    "preflight": "run-p genreg test:ts test:audit lint prettier-write",
    "prepush": "run-p genreg test lint",
    "storybook": "yarn gendocs && start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.3",
    "@mui/icons-material": "^5.4.2",
    "@mui/lab": "^5.0.0-alpha.68",
    "@mui/material": "^5.4.1",
    "@prisma/client": "^3.11.1",
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^0.26.1",
    "browserslist-useragent": "^3.1.4",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "mongodb": "^4.3.1",
    "next": "^12.1.0",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.4",
    "next-redux-wrapper": "^7.0.5",
    "next-themes": "^0.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-private-methods": "^7.16.11",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@next/eslint-plugin-next": "^12.1.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-controls": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-viewport": "^6.4.19",
    "@storybook/addons": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "17.0.17",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-preset-react-app": "^10.0.1",
    "buildcom": "^1.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-output": "^3.0.1",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-storybook": "^0.5.7",
    "eslint-plugin-testing-library": "^5.0.5",
    "full-icu": "^1.4.0",
    "husky": "^7.0.4",
    "improved-yarn-audit": "^3.0.0",
    "jest": "^27.5.1",
    "jest-html-reporter": "^3.4.2",
    "json-server": "^0.17.0",
    "lint-staged": ">=10",
    "markdown-loader": "^8.0.0",
    "markdownlint-cli": "^0.31.1",
    "mini-css-extract-plugin": "^2.6.0",
    "next-secure-headers": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prisma": "^3.11.1",
    "rimraf": "^3.0.2",
    "sass": "^1.38.0",
    "sass-loader": "^12.6.0",
    "storybook-dark-mode": "^1.0.9",
    "style-loader": "^3.2.1",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-prettier": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.5.5",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "yarn lint:ts"
    ],
    "*.json": [
      "prettier --write",
      "yarn lint:json"
    ],
    "*.{scss,css}": [
      "prettier --write",
      "yarn lint:styles"
    ],
    "*.md": [
      "prettier --write",
      "yarn lint:md"
    ]
  }
}
